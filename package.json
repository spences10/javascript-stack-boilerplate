{
  "name": "javascript-stack-boilerplate",
  "version": "0.0.1",
  "description": "javascript-stack-boilerplate",
  "main": "index.js",
  "repository": "https://github.com/spences10/javascript-stack-boilerplate.git",
  "author": "Scott Spence spences10apps@gmail.com",
  "license": "MIT",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "npm dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "heroku local",
    "heroku-postbuild": "npm prod:build",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "npm lint && flow && jest --coverage",
    "precommit": "npm test",
    "prepush": "npm test && npm prod:build"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "4.3.1",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "immutable": "4.0.0-rc.12",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "react-dom": "^16.0.0",
    "react": "^16.0.0",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-jss": "^8.0.0",
    "react-redux": "^6.0.0",
    "react-router": "next",
    "react-router-dom": "next",
    "redux": "^4.0.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.0.3",
    "socket.io": "^2.0.3",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "eslint": "5.14.1",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "fetch-mock": "7.3.0",
    "flow-bin": "0.93.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "nodemon": "1.18.10",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.3",
    "webpack": "4.29.5",
    "webpack-dev-server": "3.2.0"
  }
}
