{
  "name": "javascript-stack-boilerplate",
  "version": "0.0.1",
  "description": "javascript-stack-boilerplate",
  "main": "index.js",
  "repository": "https://github.com/spences10/javascript-stack-boilerplate.git",
  "author": "Scott Spence spences10apps@gmail.com",
  "license": "MIT",
  "engines": {
    "node": "7.x",
    "yarn": "0.20.3"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "heroku local",
    "heroku-postbuild": "yarn prod:build",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.0-beta",
    "react-hot-loader": "next",
    "react-jss": "^6.1.1",
    "react-redux": "^5.0.3",
    "react-router": "next",
    "react-router-dom": "next",
    "redux": "^3.6.0",
    "redux-actions": "2.0.2",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.0",
    "socket.io-client": "^2.0.0",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "coveralls": "^2.12.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.45.0",
    "husky": "^0.13.2",
    "jest": "^20.0.0",
    "nodemon": "^1.11.0",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.2"
  }
}
